			"override_previous_animation": true,
			"bones": {
				"bone": {
					"scale": {
						"0.0": {
							"vector": [1, 0.3, 1]
						},
						"0.8": {
							"vector": [1, 1, 1],
							"easing": "easeOutQuart"
						}
					}
				}
			}
		},
		"lightsaber_off": {
			"loop": "hold_on_last_frame",
			"animation_length": 1,
			"override_previous_animation": true,
			"bones": {
				"bone": {
					"scale": {
						"0.0": {
							"vector": [1, 1, 1]
						},
						"1": {
							"vector": [1, 0.3, 1],
							"easing": "easeInQuad"
						}
					}
				}
			}
		},
		"lightsaber_idle_off": {
			"loop": true,
			"bones": {
				"bone": {
					"scale": {
						"vector": [1, 0.3, 1]
					}
				}
			}
		}
